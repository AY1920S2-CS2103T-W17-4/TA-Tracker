@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":TaTrackerParser" as TaTrackerParser LOGIC_COLOR
participant ":AddStudentCommandParser" as AddStudentCommandParser LOGIC_COLOR
participant "d:AddStudentCommand" as AddStudentCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("add n/John Doe p/98765432 e/johnd@example.com m/A0181234G")
activate LogicManager

LogicManager -> TaTrackerParser : parseCommand("add n/John Doe p/98765432 e/johnd@example.com m/A0181234G")
activate TaTrackerParser

create AddStudentCommandParser
TaTrackerParser -> AddStudentCommandParser
activate AddStudentCommandParser

AddStudentCommandParser --> TaTrackerParser
deactivate AddStudentCommandParser

TaTrackerParser -> AddStudentCommandParser : parse("n/John Doe p/98765432 e/johnd@example.com m/A0181234G")
activate AddStudentCommandParser

create AddStudentCommand
AddStudentCommandParser -> AddStudentCommand
activate AddStudentCommand

AddStudentCommand --> AddStudentCommandParser : d
deactivate AddStudentCommand

AddStudentCommandParser --> TaTrackerParser : d
deactivate AddStudentCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
AddStudentCommandParser -[hidden]-> TaTrackerParser
destroy AddStudentCommandParser

TaTrackerParser --> LogicManager : d
deactivate TaTrackerParser

LogicManager -> AddStudentCommand : execute()
activate AddStudentCommand

AddStudentCommand -> Model : addStudent("John Doe", "98765432", "johnd@example.com", "A0181234G")
activate Model

Model --> AddStudentCommand
deactivate Model

create CommandResult
AddStudentCommand -> CommandResult
activate CommandResult

CommandResult --> AddStudentCommand
deactivate CommandResult

AddStudentCommand --> LogicManager : result
deactivate AddStudentCommand

[<--LogicManager
deactivate LogicManager
@enduml
